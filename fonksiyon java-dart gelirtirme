    function compare(value1, operator, value2) {
      switch (operator) {
          case '>':   return value1 > value2;
          case '<':   return value1 < value2;
          case '>=':  return value1 >= value2;
          case '<=':  return value1 <= value2;
          case '==':  return value1 == value2;
          case '!=':  return value1 != value2;
          case '===': return value1 === value2;
          case '!==': return value1 !== value2;
      }
      }
      
      function randomInt(min, max) {
      return Math.floor(Math.random() * (max - min + 1) + min)
      }

